{
  "compilerOptions": {
    "outDir": "build",
    "module": "commonjs",
    "target": "es2016",
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "include": [
    "src",
    "node_modules/medical_types"
  ]
}