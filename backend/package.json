{
  "name": "qshared",
  "version": "0.0.1",
  "description": "A shared music queue",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run run-watch",
    "start-test": "cross-env NODE_ENV=test npm run run-watch",
    "run": "node -r ts-node/register/type-check src/index.ts",
    "run-watch": "nodemon -r ts-node/register/type-check --inspect src/index.ts",
    "test": "cross-env NODE_ENV=test mocha -r ts-node/register \"src/**/*.test.ts\" --timeout 180000",
    "test-debug": "cross-env NODE_ENV=test mocha --inspect-brk -r ts-node/register \"src/**/*.test.ts\" --timeout 180000",
    "production": "cross-env NODE_ENV=production npm run run-watch",
    "lint-fix": "tslint --fix -c ./tslint.json 'src/**/*{.ts,.tsx}'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^3.0.5",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.1",
    "@types/sequelize": "^4.27.35",
    "@types/sinon": "^7.0.9",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.9",
    "sinon": "^7.2.7",
    "timekeeper": "^2.1.2",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "mysql2": "^1.6.5",
    "sequelize": "^4.42.0",
    "sqlite3": "^4.0.6"
  }
}
